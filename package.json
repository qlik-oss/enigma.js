{
  "name": "enigma.js",
  "version": "2.2.0",
  "description": "JavaScript library for consuming Qlik backend services",
  "author": "QlikTech International AB",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "test": "aw -c test/aw.config.js --coverage",
    "test:watch": "aw -c test/aw.config.js --coverage -w",
    "test:unit": "aw -c test/aw.config.unit.js --coverage",
    "test:unit:watch": "aw -c test/aw.config.unit.js --coverage -w",
    "test:component": "aw -c test/aw.config.comp.js --coverage",
    "test:component:watch": "aw -c test/aw.config.comp.js --coverage -w",
    "test:integration": "aw -c test/aw.config.int.js",
    "coverage": "aw -c test/aw.config.cov.js | coveralls",
    "lint": "eslint .",
    "jsdoc": "rimraf ./jsdoc && jsdoc ./src -c ./jsdoc-config.json -r -d ./jsdoc",
    "preversion": "npm run test && npm run test:integration",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/enigma.js"
  },
  "files": [
    "/enigma.js",
    "/enigma.js.map",
    "/enigma.min.js",
    "/enigma.min.js.map",
    "/sense-utilities.js",
    "/sense-utilities.js.map",
    "/sense-utilities.min.js",
    "/sense-utilities.min.js.map",
    "/schemas"
  ],
  "main": "enigma.js",
  "devDependencies": {
    "after-work.js": "github:qlik-oss/after-work.js#947fcc5",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai-subset": "1.6.0",
    "coveralls": "3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "4.12.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "extend": "3.0.1",
    "jsdoc": "3.5.5",
    "node-event-emitter": "0.0.1",
    "rimraf": "2.6.2",
    "rollup": "0.52.1",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-license": "0.5.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "ws": "3.3.2"
  },
  "dependencies": {}
}
