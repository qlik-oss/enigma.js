{
  "name": "enigma.js",
  "version": "2.0.2",
  "description": "JavaScript library for consuming Qlik backend services",
  "author": "QlikTech International AB",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "test": "cross-env NODE_ENV=test aw-test-runner cover ./test/unit ./test/component",
    "test:watch": "cross-env NODE_ENV=test aw-test-runner ./test/unit ./test/component -w",
    "test:unit": "cross-env NODE_ENV=test aw-test-runner cover ./test/unit",
    "test:unit:watch": "cross-env NODE_ENV=test aw-test-runner ./test/unit -w",
    "test:component": "cross-env NODE_ENV=test aw-test-runner cover ./test/component",
    "test:component:watch": "cross-env NODE_ENV=test aw-test-runner ./test/component -w",
    "test:integration": "cross-env NODE_ENV=test aw-test-runner ./test/integration --timeout 5000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "jsdoc": "rimraf ./jsdoc && jsdoc ./src -c ./jsdoc-config.json -r -d ./jsdoc",
    "preversion": "npm run test && npm run test:integration",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/enigma.js"
  },
  "files": [
    "/enigma.js",
    "/enigma.js.map",
    "/enigma.min.js",
    "/enigma.min.js.map",
    "/sense-utilities.js",
    "/sense-utilities.js.map",
    "/sense-utilities.min.js",
    "/sense-utilities.min.js.map",
    "/schemas"
  ],
  "main": "enigma.js",
  "devDependencies": {
    "after-work.js": "^3.0.1",
    "babel-core": "^6.10.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.7.2",
    "bluebird": "^3.4.6",
    "chai-subset": "^1.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "eslint": "4.7.2",
    "eslint-config-airbnb-base": "12.0.1",
    "eslint-plugin-import": "2.7.0",
    "extend": "^3.0.0",
    "jsdoc": "^3.5.4",
    "node-event-emitter": "0.0.1",
    "rimraf": "^2.5.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "ws": "^3.0.0"
  },
  "dependencies": {}
}
