{
  "env": {
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "rules": {
    "no-restricted-globals": ["error", "Promise"],
    "import/no-extraneous-dependencies": 0,
    "import/no-webpack-loader-syntax": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0
  }
}
